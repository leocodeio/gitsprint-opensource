{
  "name": "@gitsprint/db",
  "version": "0.0.1",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "scripts": {
    "db:gen": "npx prisma generate && npx @better-auth/cli generate --config ../../apps/web/app/server/services/auth/db.server.ts -y",
    "db:migrate": "npx prisma migrate dev && npx prisma migrate deploy",
    "db:seed": "npx tsx prisma/seed.ts",
    "build": "rm -rf dist && npx tsc"
  },
  "dependencies": {
    "@better-auth/cli": "^1.2.10",
    "@prisma/client": "^6.6.0"
  },
  "devDependencies": {
    "prisma": "^6.6.0",
    "tsx": "^4.20.5"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./dist/index.js",
      "types": "./src/index.ts"
    }
  }
}
